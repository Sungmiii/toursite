{
  "name": "toursite",
  "version": "1.0.0",
  "description": "this is sass practice website",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "complie:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all complie:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sungmiii/toursite.git"
  },
  "author": "sungmi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sungmiii/toursite/issues"
  },
  "homepage": "https://github.com/Sungmiii/toursite#readme",
  "devDependencies": {
    "autoprefixer": "^9.4.9",
    "concat": "^1.0.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}